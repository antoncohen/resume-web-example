<% name = r['name'] %>
<% title = r['title'] %>
<% email = r['email'] %>
<% phone = r['phone'] %>
<% address = r['address'] %>
<% city = r['city'] %>
<% state = r['state'] %>
<% zip = r['zip'] %>
<% blurb = r['blurb'] %>
<% passions = r['passions'] %>
<% jobs = r['jobs'] %>
<% links = r['links'] %>

 <%= name %> 
 <%= title %>

 Things I'm Passionate About:
<% passions.each do |passion| %>
  * <%= passion %>
<% end %>

<% jobs.each do |job| %> 
<%= " %-39s%39s" % [job['company'],"#{job['date_start']} - #{job['date_stop']}"] %> 
<% if job['url'] %><%= " #{job['url']}\n" %><% end %><%= " #{job['title']}" %> 
<% if job['tasks'] then job['tasks'].each do |task| %>  * <%= task.gsub(/<\/?strong>/, '') + "\n" %><% end %><% end %>
<% end %>

<% links.each do |link| %>
 <%= "#{link['name']}: #{link['url']}" %>
<% end %>

